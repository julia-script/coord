  },
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@turbo/gen": "^1.10.1",
    "next": "^13.4.4",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "turbo": "^1.10.1",
    "vitest": "^0.31.4"
  },
  "packageManager": "pnpm@7.15.0"
}
