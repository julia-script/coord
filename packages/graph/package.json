{
  "name": "@coord/graph",
  "version": "0.4.0",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.js",
  "license": "MIT",
  "scripts": {
    "lint": "tsc",
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf ./dist ./.turbo ./node_modules"
  },
  "tsup": {
    "dts": true,
    "format": [
      "cjs",
      "esm"
    ],
    "minify": true,
    "entryPoints": [
      "src/index.ts"
    ],
    "sourcemap": true
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@coord/core": "workspace:*",
    "@types/lodash-es": "^4.17.7",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "lodash-es": "^4.17.21",
    "tsconfig": "workspace:*",
    "tsup": "^6.7.0",
    "typescript": "^5.1.3",
    "vitest": "^0.31.1"
  },
  "dependencies": {
    "@use-gesture/react": "^10.2.27"
  }
}
